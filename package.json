    "lodash": "3.0.0",
    "moment": "^2.9.0",
    "nib": "1.1.0",
    "react": "0.12.2",
    "reactify": "0.17.1",
    "stylus": "0.49.3",
    "watchify": "^2.4.0"
  },
  "devDependencies": {
    "chai": "1.10.0",
    "consolidate": "0.10.0",
    "express": "4.11.0",
    "jsdom": "3.1.0",
    "mocha": "2.1.0",
    "morgan": "1.5.1",
    "node-jsx": "0.12.4",
    "sinon": "1.12.2",
