  "main": "server/index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node server",
    "debug": "DEBUG=express:* node server",
    "compile": "browserify -dt reactify src/app.jsx -o src/app.js && echo \"Compiled\"",
